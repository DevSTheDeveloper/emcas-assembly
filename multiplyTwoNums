.data
numOne: .word 0
numTwo: .word 0
p1: .asciiz "Please enter the first number: "
p2: .asciiz "Please enter the second number: "
res: .word 0

.text
.globl main
main:
    j getOne

getOne:
    li $v0, 4
    la $a0, p1
    syscall

    li $v0, 5
    syscall
    move $t0, $v0

    j getTwo

getTwo:
    li $v0, 4
    la $a0, p2
    syscall

    li $v0, 5
    syscall
    move $t1, $v0

    j multiply

multiply:
    mul $t2, $t1, $t0

    li $v0, 1
    move $a0, $t2
    syscall

    j exit

exit:
    li $v0, 10
    syscall
